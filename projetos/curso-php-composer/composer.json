{
    "name": "tadeu-proprietario/nome-pacote-curso-php-composer",
    "description": "Curso de PHP Composer",
    "authors": [
        {
            "name": "Tadeu",
            "email": "tadeu.390@gmail.com"
        }
    ],
	"minimum-stability" : "stable",
    "require": {
        "monolog/monolog": "^1.24",
		"php": ">=7",
        "dompdf/dompdf": "^0.8.3"
    },
	"require-dev": {
        "phpunit/phpunit": "^8.1"
    },
	"autoload": {
		"files": [
			"app/Helpers/functions.php"
		],
		"classmap": [
			"ClassUtils/",
			"configs/"
		],
		"psr-4": {
			"conf\\" : "src/conf/",
			"App\\" : "app/"
		}
	}
}
